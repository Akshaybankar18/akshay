package com.demo;


public class Test
{ 
	static int a=15;
	double b=a;
	double c=19.2345;
	static int i = (int)b;
			
	public static void main(String[] args)
	{
		System.out.println("Before casting"+" "+a);
        System.out.println("After casting"+" "+ b);
        		
        System.out.println("Before casting"+" "+ c);
        System.out.println("After casting"+" "+ i);
        		
	}
}
