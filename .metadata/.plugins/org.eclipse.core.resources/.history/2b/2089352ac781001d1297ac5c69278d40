package com.demo;


public class Test
{ 
	static int a=98;
	static double b=98.29475;
	static double c=34.083625;
	static int d=(int) c;
}

public static void main(String[] args ) 
{ 
	System.out.println("Before casting"+" "+ a);
	System.out.println("After casting"+" "+ b);
	System.out.println("Before casting"+" "+ c);
	System.out.println("Before casting"+" "+ d);
}
