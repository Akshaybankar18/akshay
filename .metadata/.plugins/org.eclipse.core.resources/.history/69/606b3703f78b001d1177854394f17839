package Inheritance;

private class Simple {
	void methodOne() {
		System.out.println("Parent");
	}
}

class one extends Simple {
	void methodTwo() {
		System.out.println("Child");
	}

	public static void main(String[] args) {
		one obj = new one();
		obj.methodOne();
		obj.methodTwo();

		Simple obj1 = new Simple();
		obj1.methodOne();

		Simple obj2 = new one();
		obj2.methodOne();
	}
}
